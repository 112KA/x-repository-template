{
  "name": "@112ka/x",
  "type": "module",
  "version": "0.0.10",
  "description": "軽量でモジュール化された TypeScript ユーティリティライブラリ。複数の小さなモジュール群を ES モジュールとして提供し、必要な機能を選択的にインポート可能な設計。",
  "author": "Masayuki Iizuka <mas.iizuka@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/112KA/x.git"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./*.js": {
      "types": "./dist/*.d.mts",
      "default": "./dist/*.mjs"
    }
  },
  "main": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "build": "tsdown",
    "watch": "tsdown --watch",
    "test": "vitest --run --coverage"
  },
  "devDependencies": {
    "@antfu/eslint-config": "7.4.3",
    "@eslint/eslintrc": "3.3.3",
    "@tsconfig/recommended": "1.0.13",
    "@types/webxr": "^0.5.22",
    "@types/youtube": "^0.1.2",
    "@vitest/coverage-v8": "4.0.18",
    "eslint": "10.0.0",
    "tsdown": "0.20.3",
    "vitest": "4.0.18"
  }
}
